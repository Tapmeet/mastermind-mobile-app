{
  "expo": {
    "name": "Mastermind",
    "slug": "Mastermind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#24A4DA",
      "Aspect": "fill",
      "backgroundPosition": "top center"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.martialartsschool.mastermind",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.martialartsschool.mastermind"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Best Martial Arts School in Your Town"
  }
}
